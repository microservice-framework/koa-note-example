{
  "env": {
    "es6": true
  },
"ecmaFeatures": {
        "jsx": true,
        "arrowFunctions": true,
        "blockBindings": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "restParams": true,
        "spread": true,
        "templateStrings": true,
        "modules": true,
        "classes": true
    },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "keyword-spacing": [
      2,
      {}
    ],
    "camelcase": [
      1,
      {
        "properties": "always"
      }
    ],
    "max-len": [
      2,
      100
    ]
  }
}
